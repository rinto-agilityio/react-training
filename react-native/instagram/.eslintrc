{
  "plugins": ["react", "react-native"],
  "extends": [
    "eslint:recommended",
    "roc",
    "eslint:all",
    "plugin:react/all",
    "plugin:react-native/all"
  ],
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
