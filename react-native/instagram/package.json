{
  "name": "instagram",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start NODE_ENV=development",
    "build": "node node_modules/react-native/local-cli/cli.js start NODE_ENV=production",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "updateSnapshot": "jest --updateSnapshot",
    "storybook": "storybook start -p 7007 NODE_ENV=storybook"
  },
  "dependencies": {
    "babel-eslint": "^8.0.2",
    "firebase": "^4.6.2",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-native": "0.50.3",
    "react-native-dotenv": "^0.1.0",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-image-picker": "^0.26.7",
    "react-navigation": "^1.0.0-beta.19",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.3.4",
    "reduxsauce": "^0.7.0",
    "seamless-immutable": "^7.1.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/react-native": "^3.2.16",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest": "21.2.1",
    "jest-cli": "^21.2.1",
    "prettier-eslint": "^8.2.2",
    "react-dom": "^16.1.1",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": ["**/src/**/*.test.js"],
    "transformIgnorePatterns": ["node_modules/(?!react-native|react-navigation)/"],
    "setupFiles": ["./src/test/setup.js"],
    "globals": {
      "window": {}
    },
    "collectCoverage": true,
    "coverageFormats": ["json", "html"]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
