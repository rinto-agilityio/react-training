{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ../../node_modules/react-native/local-cli/cli.js start --projectRoot ../../",
    "storybook": "storybook start -p 7007 | react-native start",
    "link": "node ../../node_modules/react-native/local-cli/cli.js link react-native-gesture-handler --verbose",
    "android": "node ../../node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node ../../node_modules/react-native/local-cli/cli.js run-ios",
    "test": "jest",
    "coverage": "jest --coverage --u --detectOpenHandles",
    "flow": "flow",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-link-firebase": "^1.1.0",
    "components": "*",
    "firebase": "^5.11.0",
    "graphql": "^14.2.1",
    "path": "^0.12.7",
    "react": "16.8.3",
    "react-apollo": "^2.5.5",
    "react-native": "0.59.6",
    "react-native-gesture-handler": "^1.2.1",
    "react-navigation": "^3.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "@storybook/react-native": "^4.1.18",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-jest-enzyme": "^7.0.2",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.98.0",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.17.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.8.3",
    "storybook": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && flow"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "eslint --cache --fix",
      "git add"
    ],
    "tests/**/*.js": [
      "jest --coverage --u",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>src/configs/jest.js",
    "testPathIgnorePatterns": [
      "<rootDir>src/configs/jest.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./storybook"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
