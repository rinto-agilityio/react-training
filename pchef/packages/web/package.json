{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "16.8.3",
    "react-art": "16.8.3",
    "react-dom": "16.8.3",
    "react-native-web": "^0.11.2",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "eject": "react-app-rewired eject",
    "eslint": "../../node_modules/.bin/eslint --ext .jsx,.js src/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "customize-cra": "^0.2.12",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-cli": "^6.26.0",
    "@babel/core": "^7.4.4",
    "@storybook/addon-actions": "4.1.18",
    "@storybook/addon-links": "4.1.18",
    "@storybook/addons": "4.1.18",
    "@storybook/react": "4.1.18",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-native-web": "^0.11.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-jest-enzyme": "^7.0.2",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.98.0",
    "flow-typed": "^2.5.1",
    "husky": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^2.1.3",
    "react-test-renderer": "^16.8.3"
  },
  "engines": {
    "node": ">= 10.15.3",
    "yarn": ">= 1.15.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && flow check"
    }
  },
  "lint-staged": {
    "{src}/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  }
}
