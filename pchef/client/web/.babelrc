{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript",
    [
      "@babel/preset-flow",
      {
        "all": true
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-react",
          {
            "development": true
          }
        ]
      ]
    }
  },
  "plugins": ["@babel/plugin-syntax-dynamic-import"]
}
